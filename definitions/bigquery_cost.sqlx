  , CAST(TIMESTAMP_ADD(TIMESTAMP(protopayload_auditlog.servicedata_v1_bigquery.jobGetQueryResultsResponse.job.jobStatistics.createTime), INTERVAL 7 HOUR) AS DATETIME) startTime
  , ROUND(((protopayload_auditlog.servicedata_v1_bigquery.jobGetQueryResultsResponse.job.jobStatistics.totalBilledBytes +
          protopayload_auditlog.servicedata_v1_bigquery.jobGetQueryResultsResponse.job.jobStatistics.totalProcessedBytes) / 1099511627776.0), 10) AS totalBilledBytes
  , CAST(TIMESTAMP_ADD(TIMESTAMP(timestamp), INTERVAL 7 HOUR) AS DATETIME) endTime
