config {
  type: "incremental",
  uniqueKey: ["stage_id"],
  schema: "sale"
}

SELECT DISTINCT * EXCEPT(row_number, _airbyte_extracted_at)
FROM (
SELECT *,
    ROW_NUMBER() OVER (PARTITION BY stage_id ORDER BY update_time DESC) AS row_number
FROM ${ref('raw_stage')}
) t
WHERE t.row_number = 1