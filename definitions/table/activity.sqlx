config {
  type: "incremental",
  uniqueKey: ["_id_"],
  schema: "sale"
}


WITH
    activity AS (
        SELECT *
        FROM ${ref('raw_activity')}
    )

SELECT DISTINCT * EXCEPT(row_number)
FROM (
SELECT *,
    ROW_NUMBER() OVER (PARTITION BY _id_ ORDER BY update_time DESC) AS row_number
FROM activity
) t
WHERE t.row_number = 1