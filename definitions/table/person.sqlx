config {
  type: "incremental",
  uniqueKey: ["person_id"],
  schema: "sale",
  bigquery: {
    partitionBy: "DATE(update_time)"
  }
}

SELECT DISTINCT * EXCEPT(row_number, _airbyte_extracted_at)
FROM (
SELECT *,
    ROW_NUMBER() OVER (PARTITION BY person_id ORDER BY _airbyte_extracted_at DESC) AS row_number
FROM ${ref('raw_person')}
) t
WHERE t.row_number = 1